import numpy as np
arr1=[2,3,4]
arr2=[6,7,8]
#bitwise and
print(np.bitwise_and(arr1,arr2)) 
#bitwise or
print(np.bitwise_or(arr1,arr2)) 
#bitwise xor
print(np.bitwise_xor(arr1,arr2)) 
#bitwise invert
print(np.invert(arr1)) 
#bitwise left shift by 2
print(np.left_shift(arr1,2))
#bitwise right shift by 2
print(np.right_shift(arr1,2))
